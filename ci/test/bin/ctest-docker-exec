set -e
CUDA_VISIBLE_DEVICES=$(echo ${CTEST_RESOURCE_GROUP_0} | sed "s/cuda//g" | sed "s/cpu,//g")
[ -z "$CUDA_VISIBLE_DEVICES" ] && echo "CUDA_VISIBLE_DEVICES empty" && exit 1
docker exec \
    -e ONEFLOW_TEST_CPU_ONLY=${ONEFLOW_TEST_CPU_ONLY} \
    -e ONEFLOW_TEST_DEVICE_NUM=${ONEFLOW_TEST_DEVICE_NUM} \
    -e CUDA_VISIBLE_DEVICES=${CUDA_VISIBLE_DEVICES} "$@"
